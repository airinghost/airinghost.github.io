{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "redirects": [
    { 
      "source": "/android-chrome-512x512.png", 
      "destination": "/favicon.svg", 
      "permanent": false 
    },
    { 
      "source": "/favicon.png", 
      "destination": "/favicon.svg", 
      "permanent": false 
    },
    { 
      "source": "/favicon.ico", 
      "destination": "/favicon.svg", 
      "permanent": false 
    },
    { 
      "source": "/posts/index.xml", 
      "destination": "/rss.xml", 
      "permanent": false 
    },
    { 
      "source": "/rss", 
      "destination": "/rss.xml", 
      "permanent": false 
    },
    { 
      "source": "/rss/", 
      "destination": "/rss.xml", 
      "permanent": false 
    },
    { 
      "source": "/feed", 
      "destination": "/rss.xml", 
      "permanent": false 
    },
    { 
      "source": "/feed/", 
      "destination": "/rss.xml", 
      "permanent": false 
    },
    { 
      "source": "/collection/", 
      "destination": "/following.html", 
      "permanent": true 
    },
    { 
      "source": "/collections/", 
      "destination": "/following.html", 
      "permanent": true 
    },
    { 
      "source": "/copies/", 
      "destination": "/following.html", 
      "permanent": true 
    },
    { 
      "source": "/me/", 
      "destination": "/about.html", 
      "permanent": true 
    },
    { 
      "source": "/notes/", 
      "destination": "/diary.html", 
      "permanent": true 
    },
    { 
      "source": "/page/", 
      "destination": "/diary.html", 
      "permanent": true 
    },
    { 
      "source": "/posts/", 
      "destination": "/diary.html", 
      "permanent": true 
    }
  ]
}